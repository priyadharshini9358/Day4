import java.util.Scanner;
public class gcd {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num1=sc.nextInt();
		int num2=sc.nextInt();
		int gcd=0;
		int min=(num1<num2)? num1:num2;
		for(int i=min;i>0;i--)
		{
			if(num1%1==0 && num2%1==0)
		{
			gcd=i;
			break;
		}
		
	}
		System.out.println(gcd);
	}
}
