import java.util.Scanner;
public class evencount {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int s=sc.nextInt();
		int e=sc.nextInt();
		int evencount=0;
		for(int i=s;i<=e;i++)
		{
			if(i%2==0)
			{
				evencount++;
				
			}
				
		}
		
		System.out.println(evencount);	
				
	}

}
