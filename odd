import java.util.Scanner;
public class odd {
		public static void main(String[] args) {
			Scanner sc=new Scanner(System.in);
			int s=sc.nextInt();
			int e=sc.nextInt();
			int oddcount=0;
			for(int i=s;i<=e;i++)
			{
				if(i%2!=0)
				{
					oddcount++;
					
				}
					
			}
			
			System.out.println(oddcount);	
					
		}

	}

